      <Expression xsi:type="q2:PythonSource">
        <q2:Name>RotaryEncoder</q2:Name>
        <q2:Description>Streams position in degrees and timestamp in seconds</q2:Description>
        <q2:Script>import clr
clr.AddReference("System")
from System import *
import struct

def rescale(value,vmin,vmax,rmin,rmax):
  return (value - vmin) / (vmax - vmin) * (rmax - rmin) + rmin

@returns(Tuple[float,float])
def generate():
  with IO.Ports.SerialPort('COM3') as ser:
    try:
      ser.BaudRate = 115200
      ser.DataBits = 8
      ser.Open()
      ser.ReadExisting()
      ser.Write('C')
      ser.ReadByte()
      ser.Write('S1')

      msg = Array[Byte](range(6))
      while True:
        while ser.BytesToRead &gt; 5:
          ser.Read(msg,0,6)
          position = rescale(BitConverter.ToInt16(msg,0),-512.,512.,-180.,180.)
          time = BitConverter.ToUInt32(msg,2) / 1000.
          yield Tuple.Create(position,time)
    finally:
      ser.Close()
